{
    "name": "headless/magento-stripe-graphql",
    "description": "Extends official Stripe Magento 2 module with additional GraphQL features",
    "type": "magento2-module",
    "version": "1.0.0",
    "license": [
        "MIT"
    ],
    "authors": [
        {
            "name": "Headless Commerce",
            "email": "go@headless.uk",
            "homepage": "https://headless.uk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.1",
        "stripe/module-payments": "^3.2"
    },
    "require-dev": {
        "outeredge/dev-dependencies": "^3.0"
    },
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "Headless\\StripeGraphQl\\": ""
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}